{
   "background": {
      "persistent": true,
      "scripts": [ "reload.js" ]
   },
  "name": "You are Fake News!",
  "version": "1.0",
  "manifest_version": 2,
  "description": "This program fact checks your friends' statuses with ",
  "permissions": ["downloads", "tabs", "*://*.twitter.com/*"],
  "content_scripts": [
    {
      "matches": ["*://*.twitter.com/*"],
      "js": ["response.js"]
    }
  ]
}