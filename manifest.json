{
   "background": {
      "persistent": true,
      "scripts": [ "reload.js" ]
   },
  "name": "You are Fake News!",
  "version": "1.0",
  "manifest_version": 2,
  "description": "This extension fact-checks your friends' tweets with a variety of news sources",
  "permissions": ["webRequest", "tabs", "*://*.twitter.com/*"],
  "content_scripts": [
    {
      "matches": ["*://*.twitter.com/*"],
      "js": ["response.js"]
    }
  ]
